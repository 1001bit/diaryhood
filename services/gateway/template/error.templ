package template

templ ErrorNotFound() {
	<html lang="en">
		<head>
			@baseHead("404")
		</head>
		<body>
			@nav("404")
			<main>
				<p>This page cannot be found or is unavailable</p>
			</main>
		</body>
	</html>
}

templ ErrorInternal() {
	<html lang="en">
		<head>
			@baseHead("500")
		</head>
		<body>
			@nav("500")
			<main>
				<p>Something went wrong!</p>
			</main>
		</body>
	</html>
}
